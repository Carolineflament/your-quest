<div class="row mt-2">
    <div class="card-list">
        <div class="card-list-head">
            <h3 class="text-muted fw-bold mb-4 text-center h3">{{title}}</h3>
        </div>
        <div class="card-list-body">
            {% for instance in instances %}
                    <div class="card card-task">
                        <div class="card-body">
                            <div class="card-title">
                                <h6><a href="#">{{instance.title}}</a></h6>
                                <span class="small">De {{instance.startAt|format_datetime(pattern="EEEE dd LLLL", locale='{{app_default_local}}')}} Ã  {{instance.endAt|format_datetime(pattern="EEEE dd LLLL", locale='{{app_default_local}}')}} : {{ game.address~' '~game.postalCode~' '~game.city }}</span>
                            </div>
                        </div>
                    </div>
                
            {% endfor %}
        </div>
    </div>
</div>